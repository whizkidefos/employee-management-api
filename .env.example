# Server Configuration
PORT=3300
NODE_ENV=development

# MongoDB Configuration
MONGODB_URI=your_mongodb_connection_string

# JWT Configuration
JWT_SECRET=your_jwt_secret
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=your_refresh_token_secret
JWT_PASSWORD_RESET_SECRET=your_password_reset_secret
JWT_PHONE_VERIFICATION_SECRET=your_phone_verification_secret

# Twilio Configuration (Optional - SMS features will be disabled if not provided)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_VERIFY_SERVICE_SID=your_twilio_verify_service_sid
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# AWS Configuration (Optional - File upload features will use local storage if not provided)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=your_aws_region
AWS_BUCKET_NAME=your_s3_bucket_name

# Email Configuration (Optional - Email features will be disabled if not provided)
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_username
SMTP_PASS=your_smtp_password
EMAIL_FROM=your_sender_email

# Stripe Configuration (Optional - Payment features will be disabled if not provided)
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Firebase Configuration (Optional - Push notifications will be disabled if not provided)
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_CLIENT_EMAIL=your_firebase_client_email
FIREBASE_PRIVATE_KEY=your_firebase_private_key

# Google Maps Configuration (Optional - Location features will be disabled if not provided)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key